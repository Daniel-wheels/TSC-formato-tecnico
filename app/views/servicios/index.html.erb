<%- model_class = Servicio -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:fecha) %></th>
      <th><%= model_class.human_attribute_name(:pedidoN) %></th>
      <th><%= model_class.human_attribute_name(:nombre) %></th>
      <th><%= model_class.human_attribute_name(:identificacion) %></th>
      <th><%= model_class.human_attribute_name(:email) %></th>
      <th><%= model_class.human_attribute_name(:direccion) %></th>
      <th><%= model_class.human_attribute_name(:telefono) %></th>
      <th><%= model_class.human_attribute_name(:tipoServicio_id) %></th>
      <th><%= model_class.human_attribute_name(:disponibilidad) %></th>
      <th><%= model_class.human_attribute_name(:descripcion) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @servicios.each do |servicio| %>
      <tr>
        <td><%= link_to servicio.id, servicio_path(servicio) %></td>
        <td><%= servicio.fecha %></td>
        <td><%= servicio.pedidoN %></td>
        <td><%= servicio.nombre %></td>
        <td><%= servicio.identificacion %></td>
        <td><%= servicio.email %></td>
        <td><%= servicio.direccion %></td>
        <td><%= servicio.telefono %></td>
        <td><%= servicio.tipoServicio_id %></td>
        <td><%= servicio.disponibilidad %></td>
        <td><%= servicio.descripcion %></td>
        <td><%=l servicio.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_servicio_path(servicio), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      servicio_path(servicio),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_servicio_path,
            :class => 'btn btn-primary' %>
