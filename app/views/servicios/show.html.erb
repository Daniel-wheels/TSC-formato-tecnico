<%- model_class = Servicio -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:fecha) %>:</strong></dt>
  <dd><%= @servicio.fecha %></dd>
  <dt><strong><%= model_class.human_attribute_name(:pedidoN) %>:</strong></dt>
  <dd><%= @servicio.pedidoN %></dd>
  <dt><strong><%= model_class.human_attribute_name(:nombre) %>:</strong></dt>
  <dd><%= @servicio.nombre %></dd>
  <dt><strong><%= model_class.human_attribute_name(:identificacion) %>:</strong></dt>
  <dd><%= @servicio.identificacion %></dd>
  <dt><strong><%= model_class.human_attribute_name(:email) %>:</strong></dt>
  <dd><%= @servicio.email %></dd>
  <dt><strong><%= model_class.human_attribute_name(:direccion) %>:</strong></dt>
  <dd><%= @servicio.direccion %></dd>
  <dt><strong><%= model_class.human_attribute_name(:telefono) %>:</strong></dt>
  <dd><%= @servicio.telefono %></dd>
  <dt><strong><%= model_class.human_attribute_name(:tipoServicio_id) %>:</strong></dt>
  <dd><%= @servicio.tipoServicio_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:disponibilidad) %>:</strong></dt>
  <dd><%= @servicio.disponibilidad %></dd>
  <dt><strong><%= model_class.human_attribute_name(:descripcion) %>:</strong></dt>
  <dd><%= @servicio.descripcion %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              servicios_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_servicio_path(@servicio), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              servicio_path(@servicio),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
