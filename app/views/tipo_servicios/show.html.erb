<%- model_class = TipoServicio -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:nombre) %>:</strong></dt>
  <dd><%= @tipo_servicio.nombre %></dd>
</dl>

<%= link_to t('.atras', :default => t("helpers.links.atras")),
              tipo_servicios_path, :class => 'btn btn-default'  %>
<%= link_to t('.editar', :default => t("helpers.links.editar")),
              edit_tipo_servicio_path(@tipo_servicio), :class => 'btn btn-default' %>
<%= link_to t('.eliminar', :default => t("helpers.links.eliminar")),
              tipo_servicio_path(@tipo_servicio),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
